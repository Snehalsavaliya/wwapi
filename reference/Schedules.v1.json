          "content": {
            "application/json": {
              "schema": {
                "description": "",
                "type": "object",
                "properties": {
                  "auth": {
                    "type": "object",
                    "properties": {
                      "applicationid": {
                        "type": "string",
                        "minLength": 1
                      },
                      "userid": {
                        "type": "string",
                        "minLength": 1
                      },
                      "password": {
                        "type": "string",
                        "minLength": 1
                      }
                    },
                    "required": [
                      "applicationid",
                      "userid",
                      "password"
                    ]
                  }
                },
                "required": [
                  "auth"
                ]
              },
              "examples": {
                "example-1": {
                  "value": {
                    "auth": {
                      "applicationid": "JobMgtTool",
                      "userid": "user",
                      "password": "pass"
                    },
                    "device": "CenterId1:DeviceId1"
                  }
                }
              }
            }
          }
        },
        "description": "Get devices from schdule  "
      },
      "patch": {
        "summary": "Delete devices from schedule",
        "operationId": "patch-shedules-shedulesId",
        "responses": {
          "202": {
            "description": "Accepted"
          },
          "401": {
            "description": "Unauthorized"
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "description": "",
                "type": "object",
                "properties": {
                  "auth": {
                    "type": "object",
                    "required": [
                      "applicationid",
                      "userid",
                      "password"
                    ],
                    "properties": {
                      "applicationid": {
                        "type": "string",
                        "minLength": 1
                      },
                      "userid": {
                        "type": "string",
                        "minLength": 1
                      },
                      "password": {
                        "type": "string",
                        "minLength": 1
                      }
                    }
