                      "items": {
                        "type": "string"
                      }
                    }
                  },
                  "required": [
                    "schedule",
                    "devices"
                  ]
                },
                "examples": {
                  "example-1": {
                    "value": {
                      "schedule": "1",
                      "devices": [
